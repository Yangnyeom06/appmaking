<MainWindow>:
    name: "main"

    startStation: _startStation
    startStationInput: _startStationInput
    arriveStation: _arriveStation
    arriveStationInput: _arriveStationInput

    FloatLayout:

        Label:
            id: _startStation
            text: '출발역'
            font_size: 15
            size_hint: None, None
            size: 150, 100
            pos: 0, 250
            font_name: 'nanummyeongjoEB'

        Button:
            id: _startStationInput
            text: '--'
            mulitiline: False
            font_size: 30
            size_hint: None, None
            size: 250, 100
            pos: 150, 250
            font_name: 'nanummyeongjoEB'
            on_release:
                app.root.current = "ListStation"
                root.manager.transition.direction = "right"

        Label:
            id: _arriveStation
            text: '도착역'
            font_size: 15
            size_hint: None, None
            size: 150, 100
            pos: 0, 150
            font_name: 'nanummyeongjoEB'

        Button:
            id: _arriveStationInput
            text: '--'
            password: False
            font_size: 30
            size_hint: None, None
            size: 250, 100
            pos: 150, 150
            font_name: 'nanummyeongjoEB'
            on_release:
                app.root.current = "ListStation"
                root.manager.transition.direction = "right"

        Button:
            text: 'Submit'
            font_size: 30
            size_hint: None, None
            size: 250, 100
            pos: 400, 0
            font_name: 'nanummyeongjoEB'
            on_release:
                app.root.current = "third"
                root.solo_submit()
                root.manager.transition.direction = "left"

        Button:
            text: "Go for two"
            font_size: 30
            size_hint: None, None
            size: 250, 100
            pos: 150, 0
            font_name: 'nanummyeongjoEB'
            on_release:
                app.root.current = "second"
                root.manager.transition.direction = "left"









